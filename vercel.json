{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_APP_NAME": "NMS Dashboard"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://adityaraj3136.github.io/"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://adityaraj3136.github.io/"
        }
      ]
    }
  ]
}
